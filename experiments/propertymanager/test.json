{
    "memory": 1000,
    "scripting": {
        "onstart": [
            {"log": ["Hello, world"]},
            {"log": ["Hello, world!"]}
        ],
        "onchat": [
            {"if": ["chatType == CHAT::NORMAL"]},
            {"if": ["stringReceived == \"hi\""]},
            {"log": ["we talked saying ${1}!"]},
            {"set": [0, "hello"]},
            {"nl": []},
            {"eval": ["jjPlayers[1].name = \"*{1}\";\n"]},
            {"endif": []},
            {"set": [1, "and it will come"]},
            {"eval": ["jjPlayers[2].name = \"*{2}\";\n"]},
            {"endif": []},
            {"eval": ["jjPlayers[3].health = 0;\n"]}
        ]
    },
    "weapons": {
        "blaster": {
            "infinite": false,
            "spread": "ICE",
            "style": "MISSILE"
        }
    }
}
